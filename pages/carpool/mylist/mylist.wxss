@import '../../../components/loading/loading.wxss';
@import '../../../components/tips/tips.wxss';
.my-list{
  background:#f5f5f5;
}
.my-item{
  padding:0 20rpx;
  background:#fff;
  margin-bottom:20rpx;
}
.my-item>navigator:nth-of-type(1){
  font-size:32rpx;
  height:92rpx;
  line-height:92rpx;
  color:#333;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.my-item>navigator:nth-of-type(2){
  font-size:24rpx;
  color:#ccc;
  padding-bottom:40rpx;
  position:relative;
}
.my-item>navigator:nth-of-type(2) text:nth-of-type(2){
  margin:0 20rpx;
}
.my-item>navigator:nth-of-type(2)::after{
  content:'';
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 1px;
  background-color: #f0f0f0;
}

.contorll{
  align-items: center;
  height:70rpx;
}
.contorll view,.contorll navigator,.contorll button{
  flex:1;
  color:#666;
  font-size:28rpx;
  text-align: center;
  position: relative;
}
.contorll button::after{
  border:none
}
.contorll button::before{
  content:'';
  position: absolute;
  right: 0;top:24rpx;
  height: 20rpx;
  width: 1px;
  background: #f0f0f0;
  transform: scaleX(0.5);
}
.contorll navigator::before{
  content:'';
  position: absolute;
  right: 0;top:8rpx;
  height: 20rpx;
  width: 1px;
  background: #f0f0f0;
  transform: scaleX(0.5);
}
.contorll view::before{
  content:'';
  position: absolute;
  right: 0;top:8rpx;
  height: 20rpx;
  width: 1px;
  background: #f0f0f0;
  transform: scaleX(0.5);
}
.nothing{
  padding:16rpx 0;
  height:80rpx;
  line-height:80rpx;
  font-size:28rpx;
  color:#666;
  background:#fefefe;
  text-align:center
}