.bottoms{
  position:fixed;
  bottom:0;
  width: 100%;
  height:100rpx;
  background:#f5f5f5;
  display: flex;
}
.nav{
  flex:1;
  font-size:24rpx;
  display: flex;
  position: relative;
  justify-content:center;
  height:100%;
}
.middle text{
  position: absolute;
  top:52rpx;
  color:#fff;
  z-index:1;
  display: inline-block;
}
.left,.right{
  color:#666;
  align-items: center;
  justify-content:center;
  flex-direction:column;
}
.bgi{
  width:120rpx;
  height:100rpx;
  position:absolute;
  top:-8rpx;
  display: inline-block;
}