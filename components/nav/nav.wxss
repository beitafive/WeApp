.bottoms{
  position:fixed;
  bottom:0;
  width: 100%;
  height:100rpx;
  background:#f5f5f5;
  display: flex;
}
.nav{
  flex:1;
  font-size:24rpx;
  display: flex;
  position: relative;
  justify-content:center;
  height:100%;
  color:#666;
  align-items: center;
  flex-direction:column;
}
.nav image{
  width:40rpx;
  height:40rpx;
  margin:8rpx 0;
}
